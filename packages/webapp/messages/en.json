  "BATCH_TAG_ADD_MODAL_ERROR_ADD_FAILED": "Failed to add tags: {errorDetails}",
  "BATCH_TAG_ADD_MODAL_ERROR_NO_BOOKMARKS_SELECTED": "No bookmarks selected",
  "BATCH_TAG_ADD_MODAL_SELECTED_BOOKMARKS_COUNT": [
    {
      "declarations": [
        "input count",
        "local countPlural = count: plural"
      ],
      "selectors": [
        "countPlural"
      ],
      "match": {
        "countPlural=one": "{count} bookmark selected",
        "countPlural=other": "{count} bookmarks selected"
      }
    }
  ],
  "BATCH_TAG_ADD_MODAL_SUCCESS_MESSAGE": [
    {
      "declarations": [
        "input bookmarksCount",
        "input tagsCount",
        "local bookmarksPlural = bookmarksCount: plural",
        "local tagsPlural = tagsCount: plural"
      ],
      "selectors": [
        "bookmarksPlural",
        "tagsPlural"
      ],
      "match": {
        "bookmarksPlural=one, tagsPlural=one": "Successfully added {tagsCount} tag to {bookmarksCount} bookmark",
        "bookmarksPlural=one, tagsPlural=other": "Successfully added {tagsCount} tags to {bookmarksCount} bookmark",
        "bookmarksPlural=other, tagsPlural=one": "Successfully added {tagsCount} tag to {bookmarksCount} bookmarks",
        "bookmarksPlural=other, tagsPlural=other": "Successfully added {tagsCount} tags to {bookmarksCount} bookmarks"
      }
    }
  ],
  "BATCH_TAG_ADD_MODAL_TITLE": "Batch Add Tags",
  "BATCH_TAG_REMOVE_MODAL_ERROR_NO_TAGS_SELECTED": "Please select at least one tag to remove",
  "BATCH_TAG_REMOVE_MODAL_ERROR_REMOVE_FAILED": "Failed to remove tags: {errorDetails}",
  "BATCH_TAG_REMOVE_MODAL_SUCCESS_MESSAGE": [
    {
      "declarations": [
        "input bookmarksCount",
        "input tagsCount",
        "local bookmarksPlural = bookmarksCount: plural",
        "local tagsPlural = tagsCount: plural"
      ],
      "selectors": [
        "bookmarksPlural",
        "tagsPlural"
      ],
      "match": {
        "bookmarksPlural=one, tagsPlural=one": "Successfully removed {tagsCount} tag from {bookmarksCount} bookmark",
        "bookmarksPlural=one, tagsPlural=other": "Successfully removed {tagsCount} tags from {bookmarksCount} bookmark",
        "bookmarksPlural=other, tagsPlural=one": "Successfully removed {tagsCount} tag from {bookmarksCount} bookmarks",
        "bookmarksPlural=other, tagsPlural=other": "Successfully removed {tagsCount} tags from {bookmarksCount} bookmarks"
      }
    }
  ],
  "BATCH_TAG_REMOVE_MODAL_SUCCESS_MESSAGE_WITH_DELETIONS": [
    {
      "declarations": [
        "input bookmarksCount",
        "input tagsCount",
        "input deletedBookmarksCount",
        "local bookmarksPlural = bookmarksCount: plural",
        "local tagsPlural = tagsCount: plural",
        "local deletedBookmarksPlural = deletedBookmarksCount: plural"
      ],
      "selectors": [
        "bookmarksPlural",
        "tagsPlural",
        "deletedBookmarksPlural"
      ],
      "match": {
        "bookmarksPlural=one, tagsPlural=one,deletedBookmarksPlural=one": "Successfully removed {tagsCount} tag from {bookmarksCount} bookmark, and {deletedBookmarksCount} bookmark was deleted as a result.",

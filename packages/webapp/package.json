{
  "name": "utags-app",
  "version": "0.28.4-2025100201",
  "author": "Pipecraft",
  "namespace": "https://utags.pipecraft.net/",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/utags/utags/issues"
  },
  "homepage": "https://github.com/utags/utags#readme",
  "type": "module",
  "scripts": {
    "xo": "xo",
    "lint": "run-s lint:*",
    "lint:format": "pnpm -w run format",
    "lint:code": "xo --fix",
    "lint:type": "tsc --noemit",
    "lint:check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite",
    "dev:proxy": "tsx watch mock-server/webdav-proxy.ts",
    "build": "vite build",
    "preview": "vite preview",
    "package": "cd dist && zip -r ../utags-webapp.zip . && cd ..",
    "test": "vitest --run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --coverage.enabled=true",
    "test:real": "USE_REAL_API=true vitest",
    "mock:sync-server": "node --loader ts-node/esm mock-server/sync-server.ts",
    "compile-paraglide": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide",
    "machine-translate": "inlang machine translate --project project.inlang",
    "sort-messages-keys": "node sort-message-keys.js"
  },
  "devDependencies": {
    "@inlang/cli": "^3.0.12",
    "@inlang/paraglide-js": "2.1.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@tailwindcss/vite": "^4.1.16",
    "@testing-library/svelte": "^5.2.8",
    "@types/express": "^5.0.5",
    "autoprefixer": "^10.4.21",
    "browser-extension-utils": "^0.2.0",
    "console-tagger": "^0.0.6",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "focus-trap-lite": "^0.0.6",
    "http-proxy-middleware": "^3.0.0",
    "jsdom": "^27.4.0",
    "lucide-svelte": "^0.552.0",
    "msw": "^2.11.6",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "svelte": "5.35.7",
    "svelte-check": "^4.3.3",
    "svelte-persisted-store": "^0.12.0",
    "svelte-virtual-list": "^3.0.1",
    "tailwindcss": "^4.1.16",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "utags-utils": "^0.3.3",
    "vite": "^7.1.12",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^3.2.4"
  }
}
